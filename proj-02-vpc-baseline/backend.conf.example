## Example backend.conf for `terraform init -backend-config=backend.conf`
# Copy this to a file named `backend.conf`, fill the values, and keep it out of source control.

bucket = "vpc-terraform-state-<ACCOUNT_ID>"    # e.g. vpc-terraform-state-123456789012
key    = "vpc/terraform.tfstate"
region = "us-east-1"
encrypt = true
dynamodb_table = "vpc-terraform-locks"

# Optional: specify a profile if you use named AWS CLI profiles
#profile = "your-aws-profile"
